#include <iostream>
#include <iomanip>
#include <string.h>
using namespace std;

int main() {
    double grade_a, grade_b, grade_c, weight_a, weight_b, weight_c, average;
    cin >> grade_a;
    cin >> grade_b;
    cin >> grade_c;
    weight_a = 2;
    weight_b = 3;
    weight_c = 5;
    average = (grade_a * weight_a + grade_b * weight_b + grade_c * weight_c)/(weight_a + weight_b + weight_c);
    cout << "MEDIA = " << fixed << setprecision(1) << average << "\n";
    return 0;
}
